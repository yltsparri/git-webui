  "jest": {
    "coverageReporters": [
      "json"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "ts",
      "tsx"
    ],
    "transform": {
      "^.+\\.(ts|tsx|js|jsx)$": "<rootDir>/node_modules/typescript-babel-jest"
    },
    "globals": {
      "__TS_CONFIG__": {
        "module": "es6",
        "jsx": "preserve",
        "target": "es6",
        "moduleResolution": "node",
        "baseUrl": "src",
        "outDir": "ts-build"
      }
    },
    "transformIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "verbose": true,
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(ts|tsx|js|jsx)$"
  }
